# WokGen apps/web - example env file (placeholders only).
# Copy this to .env.local and fill values for local testing.
# Do NOT commit .env.local with real secrets.

# Local SQLite DB (dev): use file:./dev.db for local development
DATABASE_URL="file:./dev.db"

# Base URL used by Next.js metadata and sitemap generation
NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# Disable Next telemetry if you prefer
NEXT_TELEMETRY_DISABLED=1

# Optional provider keys (uncomment and set only for testing; do not commit)
# TOGETHER_API_KEY="sk-xxxxx"
# REPLICATE_API_TOKEN="r8.xxxxx"
# FAL_API_KEY="fl_xxxxxx"

# ComfyUI local host/port (if using local ComfyUI)
# COMFYUI_HOST="http://localhost"
# COMFYUI_PORT=8188

# Misc (optional)
# NEXTAUTH_SECRET="some_long_random_secret_for_nextauth_if_enabled"
