# WokGen apps/web - example env file (placeholders only).
# Copy this to .env.local and fill values for local development.
# Do NOT commit .env.local with real secrets.

# Database: PostgreSQL is required (schema uses postgresql provider).
# Options for local dev:
#   1. Free Neon DB: https://neon.tech → create project → copy connection string
#   2. Local Docker: docker run -d -p 5432:5432 -e POSTGRES_USER=wokgen -e POSTGRES_PASSWORD=dev -e POSTGRES_DB=wokgen postgres:15
DATABASE_URL="postgresql://wokgen:dev@localhost:5432/wokgen"

# Base URL used by Next.js metadata and sitemap generation
NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# Disable Next telemetry if you prefer
NEXT_TELEMETRY_DISABLED=1

# Required for NextAuth v5
# Generate with: openssl rand -base64 32
# AUTH_SECRET="some_long_random_secret_here"
# AUTH_GITHUB_ID="your_github_oauth_app_client_id"
# AUTH_GITHUB_SECRET="your_github_oauth_app_client_secret"
# GitHub OAuth App: https://github.com/settings/developers → New OAuth App
# Callback URL: http://localhost:3000/api/auth/callback/github
# AUTH_GOOGLE_ID="your_google_oauth_client_id"
# AUTH_GOOGLE_SECRET="your_google_oauth_client_secret"
# Google OAuth: https://console.cloud.google.com/apis/credentials → Create OAuth 2.0 Client
# Authorized redirect URI: http://localhost:3000/api/auth/callback/google

# Resend — transactional email (optional for local dev, required in production)
# Sign up at https://resend.com, add wokspec.org domain, get API key
# RESEND_API_KEY="re_xxxxx"

# Admin panel access (must match the GitHub account email for the admin user)
# ADMIN_EMAIL="your@email.com"

# HD generation via Replicate (optional — standard quality uses Pollinations, no key needed)
# REPLICATE_API_TOKEN="r8.xxxxx"

# HuggingFace — standard voice TTS via Kokoro-82M (optional — falls back to browser TTS if unset)
# HF_TOKEN="hf_xxxxx"

# ElevenLabs — primary TTS provider, highly recommended for human-quality voices
# Free tier: 10k chars/month — https://elevenlabs.io/sign-up
# ELEVENLABS_API_KEY=

# OpenAI — secondary TTS + text generation
# $0.015/1K chars for TTS-HD — https://platform.openai.com/api-keys
# OPENAI_API_KEY=

# Groq API - free at console.groq.com - required for Eral AI chat and Text studio
# GROQ_API_KEY="gsk_xxxxx"

# Together AI — LLM fallback for Text studio (optional)
# TOGETHER_API_KEY="xxxxx"

# Stripe billing (optional for local dev — billing features disabled if unset)
# STRIPE_SECRET_KEY="sk_test_xxxxx"
# STRIPE_WEBHOOK_SECRET="whsec_xxxxx"
# STRIPE_PRICE_ID_PLUS="price_xxxxx"
# STRIPE_PRICE_ID_PRO="price_xxxxx"
# STRIPE_PRICE_ID_MAX="price_xxxxx"
# STRIPE_PRICE_ID_CREDITS_MICRO="price_xxxxx"
# STRIPE_PRICE_ID_CREDITS_SMALL="price_xxxxx"
# STRIPE_PRICE_ID_CREDITS_MEDIUM="price_xxxxx"
# STRIPE_PRICE_ID_CREDITS_LARGE="price_xxxxx"

# ComfyUI local host/port (if using local ComfyUI in self-hosted mode)
# COMFYUI_HOST="http://localhost"
# COMFYUI_PORT=8188

# Set to "true" to enable self-hosted BYOK mode (disables auth/billing)
# NEXT_PUBLIC_SELF_HOSTED=false

# ── AI / Generation APIs ──────────────────────────────────────────────────────

# Ideogram — image generation (optional)
# https://ideogram.ai/manage-api
# IDEOGRAM_API_KEY=

# Meshy — text-to-3D model generation (optional)
# https://www.meshy.ai/api
# MESHY_API_KEY=

# Skybox by Blockade Labs — 360° skybox / panorama generation (optional)
# https://skybox.blockadelabs.com/
# SKYBOX_API_KEY=

# AssemblyAI — audio transcription (optional)
# https://www.assemblyai.com/dashboard/signup
# ASSEMBLYAI_API_KEY=

# Vectorizer.AI — raster-to-SVG vectorization (optional)
# https://vectorizer.ai/api
# VECTORIZER_API_ID=
# VECTORIZER_API_SECRET=

# Recraft — vector/icon generation (optional)
# https://www.recraft.ai/
# RECRAFT_API_KEY=

# Cerebras — ultra-fast LLM inference (optional, used for Eral chat fallback)
# https://cloud.cerebras.ai/
# CEREBRAS_API_KEY=

# Exa — semantic web search (optional, used by Eral research tools)
# https://exa.ai/
# EXA_API_KEY=

# Firecrawl — web scraping / crawling (optional)
# https://www.firecrawl.dev/
# FIRECRAWL_API_KEY=

# Anthropic Claude (optional, LLM alternative)
# https://console.anthropic.com/
# ANTHROPIC_API_KEY=

# Google Gemini (optional, LLM alternative)
# https://aistudio.google.com/app/apikey
# GOOGLE_AI_API_KEY=

# Mistral (optional, LLM alternative)
# https://console.mistral.ai/
# MISTRAL_API_KEY=

# Cohere (optional, LLM alternative)
# https://dashboard.cohere.com/
# COHERE_API_KEY=

# fal.ai — fast image generation (optional)
# https://fal.ai/
# FAL_KEY=

# Stable Horde — decentralized open-source image generation (optional, free)
# https://stablehorde.net/register
# STABLE_HORDE_KEY=

# Freesound — sound effects library (optional)
# https://freesound.org/apiv2/apply
# FREESOUND_API_KEY=

# ── CDN / Object Storage ──────────────────────────────────────────────────────

# S3-compatible CDN for asset storage (optional — assets stored locally if unset)
# CDN_ENDPOINT="https://s3.us-east-1.amazonaws.com"
# CDN_REGION="us-east-1"
# CDN_ACCESS_KEY_ID=
# CDN_SECRET_ACCESS_KEY=
# CDN_BUCKET="wokgen-assets"
# CDN_PUBLIC_URL="https://cdn.wokspec.org"

# ── Redis / Queue ─────────────────────────────────────────────────────────────

# Redis URL for BullMQ job queue (optional — jobs run in-process if unset)
# REDIS_URL="redis://localhost:6379"
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=
# BULL_MQ_ENABLED=false

# ── Observability ─────────────────────────────────────────────────────────────

# Axiom — structured log ingestion (optional)
# https://axiom.co/
# AXIOM_DATASET=
# AXIOM_TOKEN=
# LOG_LEVEL="info"

# ── Discord Bot (optional) ────────────────────────────────────────────────────

# DISCORD_PUBLIC_KEY=

# ── Worker / Generation Tuning ────────────────────────────────────────────────
# These have safe defaults and rarely need changing.

# GENERATION_CONCURRENCY=4
# GENERATION_TIMEOUT_MS=30000
# MAX_BATCH_SIZE=10
# RATE_LIMIT_ENABLED=true
# ENABLE_QUALITY_GATE=true
# DISABLE_GUEST_GENERATION=false
# DISABLE_SIGNUPS=false
# MAINTENANCE_MODE=false
# WORKER_MODE=inline
# WORKER_CONCURRENCY=4
# WORKER_SECRET=
# CRON_SECRET=
# METRICS_SECRET=
# INTERNAL_API_URL="http://localhost:3000"
# PROMPT_ENGINE=default

# ComfyUI (optional — for self-hosted local generation)
# COMFYUI_URL="http://localhost:8188"
# COMFYUI_CHECKPOINT="dreamshaper_8.safetensors"
